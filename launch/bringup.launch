<launch>

     <include file="$(find gazebo_ros)/launch/empty_world.launch">
          <arg name="world_name" value="$(find motion)/worlds/bookstore.world"/>
          <arg name="paused" value="false"/>
          <arg name="use_sim_time" value="true"/>
          <arg name="gui" default="false"/>
          <arg name="headless" value="false"/>
          <arg name="debug" value="false"/>
          <arg name="verbose" value="false"/>
     </include>

     <!-- Launch Robot -->
     <include file="$(find hera_description)/launch/load_description.launch">
          <arg name="robot_model" default="hera_full"/>
          <arg name="use_jsp_gui" default="false"/>
     </include>
     
     <include file="$(find motion)/launch/spawn.launch"/>
</launch>